{
  "name": "graphen",
  "version": "1.10.19",
  "description": "Graphen is a small library, that keeps reusable blocks of UI and helps making application design consistent across multiple projects.",
  "main": "src/index.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "prepare": "webpack --mode=production --config webpack.prod.config.js",
    "build:dev": "webpack --mode=development --config webpack.dev.config.js",
    "build:prod": "webpack --mode=production --config webpack.prod.config.js",
    "build:example": "webpack --mode=production --config webpack.example.config.js",
    "lint": "eslint --config .eslintrc --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --config .eslintrc --ext .ts,.tsx ./src",
    "csslint": "stylelint \"./**/*.scss\" --config .stylelintrc",
    "csslint:fix": "stylelint \"./**/*.scss\" --fix --config .stylelintrc",
    "pretty": "prettier \"./src/**/*.{js,jsx,ts,tsx}\" \"./example-server/**/*.{js,jsx,ts,tsx}\" --list-different",
    "pretty:fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\" \"./example-server/**/*.{js,jsx,ts,tsx}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "example-run": "node ./example-server/index.js",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.6.0",
    "cypress": "^13.6.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-codait": "^1.1.3",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^5.1.0",
    "flow-bin": "^0.94.0",
    "koa": "^2.13.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^0.13.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "url-loader": "^3.0.0",
    "typescript": "^4.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "author": "Oskar Szura",
  "license": "ISC"
}
