{
  "name": "graphen",
  "version": "1.8.5",
  "description": "Graphen is a small library, that keeps reusable blocks of UI and helps making application design consistent across multiple projects.",
  "main": "dist/scripts.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "prepare": "webpack --mode=production --config webpack.prod.config.js",
    "build:dev": "webpack --mode=development --config webpack.dev.config.js",
    "build:prod": "webpack --mode=production --config webpack.prod.config.js",
    "build:example": "webpack --mode=production --config webpack.example.config.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix .",
    "csslint": "stylelint \"./**/*.scss\" --config .stylelintrc",
    "csslint:fix": "stylelint \"./**/*.scss\" --fix --config .stylelintrc",
    "flow": "flow",
    "pretty": "prettier \"./src/**/*.{js,jsx}\" \"./example-server/**/*.{js,jsx}\" --list-different",
    "pretty:fix": "prettier --write \"./src/**/*.{js,jsx}\" \"./example-server/**/*.{js,jsx}\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "example-run": "node ./example-server/index.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "koa": "^2.13.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "cypress": "^3.8.3",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-codait": "^1.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^5.1.0",
    "flow-bin": "^0.94.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.13.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "author": "Oskar Szura",
  "license": "ISC"
}
